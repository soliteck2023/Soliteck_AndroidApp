<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <string name="Widget.MaterialComponents.TextInputLayout.OutlinedBox"></string>
    <!--<style name="styleA">-->
    <!--    <item name="android:fontFamily">@font/bungee_outline</item>-->
    <!--    <item name="android:textStyle">normal</item>-->
    <!--    <item name="android:textColor">?android:attr/actionBarSize</item>-->
    <!--</style>-->

    <style name="customfontstyle" parent="@android:style/TextAppearance.Small">
        <!--        <item name="android:fontFamily"> </item>-->
    </style>
    <style name="AppTheme_PopupOverlay" parent="@style/ThemeOverlay.AppCompat.Light"></style>
    <style name="AppTheme_AppBarOverlay" parent="@style/ThemeOverlay.AppCompat.Dark.ActionBar">
    </style>
    <style name="AppTheme" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <!--        <item name="android:fontFamily">sans-serif-black</item>-->
        <item name="android:windowContentTransitions">true</item>
        <item name="android:windowEnterTransition">@transition/change_image_transform</item>
        <item name="android:windowExitTransition">@transition/change_image_transform</item>
        <item name="android:windowSharedElementEnterTransition">@transition/change_image_transform</item>
        <item name="android:windowSharedElementExitTransition">@transition/change_image_transform</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>
    <style name="AppTheme_NoActionBar" parent="@style/AppTheme">
        <!--        <item name="android:fontFamily">@font/sans-serif-black</item>-->
        <item name="android:windowContentTransitions">true</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="CustomAlertDialogStyle">
        <item name="android:fullDark">@color/transparent</item>
        <item name="android:topDark">@color/transparent</item>
        <item name="android:centerDark">@color/transparent</item>
        <item name="android:bottomDark">@color/transparent</item>
        <item name="android:fullBright">@color/transparent</item>
        <item name="android:topBright">@color/transparent</item>
        <item name="android:centerBright">@color/transparent</item>
        <item name="android:bottomBright">@color/transparent</item>
        <item name="android:bottomMedium">@color/transparent</item>
        <item name="android:centerMedium">@color/transparent</item>
    </style>


    <style name="RedButtonStyle">
        <item name="android:background">@drawable/red_button_background</item>
        <item name="android:textColor">#FFFFFF</item>
        <!-- You can customize other attributes as needed -->
    </style>
    <style name="myDialogTheme" parent="@android:style/Theme.Holo.Dialog" tools:ignore="DuplicateDefinition">
        <item name="android:textColorPrimary">@color/colorPrimary</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:alertDialogStyle">@style/CustomAlertDialogStyle</item>
        <item name="android:textSize">16sp</item>
<!--        <item name="android:textStyle">1x1</item>-->
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:backgroundDimEnabled">true</item>
<!--        <item name="android:fontFamily">@font/questrial</item>-->
    </style>
    <style name="DialogTheme" parent="@style/Theme.AppCompat.Light.NoActionBar">
<!--        <item name="android:fontFamily">@font/questrial</item>-->
        <item name="colorAccent">@color/colorPrimary</item>
    </style>


</resources>